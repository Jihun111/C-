#pragma warning(disable:4996)
#include<stdio.h>
int input(void);
int main()
{
	int num;
	int i = 1;
	num = input();
	while (num != 0) {
		printf("*");
		if (i % 5 ==0) {
			printf("\n");
		}
		else {
			;
		}
		num--;
		i++;
	}

}
int input(void) {
	int num;
	printf("# 정수값을 입력하세요 :");
	scanf("%d", &num);
	return num;
}
